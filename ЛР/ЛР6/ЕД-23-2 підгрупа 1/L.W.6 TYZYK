SELECT [EMPL_NUM], [NAME], [AGE], [TITLE]
FROM [dbo].[SALESREPS]
WHERE AGE > 45
ORDER BY [AGE] DESC;
GO

SELECT DISTINCT [MFR], [PRODUCT]
FROM [dbo].[ORDERS] 
WHERE ORDER_DATE BETWEEN '20080101' AND '20081231';
GO

SELECT TOP 1 WITH TIES [ORDER_NUM], [REP], COUNT (ORDER_NUM) AS [NUM_REP]
FROM [dbo].[ORDERS]
WHERE ORDER_DATE BETWEEN '20080101' AND '20081231'
GROUP BY [REP], [ORDER_NUM]
ORDER BY [ORDER_NUM];
GO
